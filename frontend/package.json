{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "author": "PDB 4",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "./scripts/start.sh",
    "prod": "./scripts/start.prod.sh",
    "start:js": "react-scripts start",
    "build:js": "react-scripts build",
    "test:js": "react-scripts test",
    "clean": "rm -rf ./node_modules",
    "lint": "cross-env npm-run-all lint:eslint",
    "lint:eslint": "./node_modules/.bin/eslint . --ext .js,.jsx,.ts,.tsx --ignore-pattern node_modules/",
    "lint:prettier": "prettier \"*.{js,jsx}\" --print-width 80 --trailing-comma es5 --write"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "dependencies": {
    "axios": "^0.19.0",
    "history": "^4.10.1",
    "qrcode.react": "^1.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-qr-reader": "^2.2.1",
    "react-router-dom": "^5.1.2",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "husky": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.19.1",
    "react-scripts": "3.2.0"
  }
}
